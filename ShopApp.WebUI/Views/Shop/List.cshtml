<div class="row">
    <div class="col-md-3">
        @await Component.InvokeAsync("CategoryList")
    </div>
    <div class="col-md-9">

        <div class="row">

            @foreach (var ticket in Model.Tickets)
            {
                <partial name="_ticket" for="@ticket" />
            }
        </div>
        <div page-model="@Model.PagingInfo"></div>

    </div>
</div>

